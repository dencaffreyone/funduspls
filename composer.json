{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "gedmo/doctrine-extensions": "^2.4",
        "sensio/framework-extra-bundle": "^5.1",
        "sonata-project/admin-bundle": "3.51.*",
        "sonata-project/core-bundle": "3.17.*",
        "sonata-project/doctrine-orm-admin-bundle": "3.9.*",
        "theofidry/alice-data-fixtures": "1.1.*",
        "symfony/webpack-encore-bundle": "1.6.*",
        "symfony/swiftmailer-bundle": "3.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/serializer-pack": "*",

        "knplabs/doctrine-behaviors": "1.6.*",
        "a2lix/translation-form-bundle": "3.0.*",

        "symfony/flex": "1.4.*",

        "symfony/asset": "4.4.*@dev",
        "symfony/console": "4.4.*@dev",
        "symfony/dotenv": "4.4.*@dev",
        "symfony/expression-language": "4.4.*@dev",
        "symfony/form": "4.4.*@dev",
        "symfony/framework-bundle": "4.4.*@dev",
        "symfony/process": "4.4.*@dev",
        "symfony/security-bundle": "4.4.*@dev",
        "symfony/translation": "4.4.*@dev",
        "symfony/twig-bundle": "4.4.*@dev",
        "symfony/validator": "4.4.*@dev",
        "symfony/web-link": "4.4.*@dev",
        "symfony/yaml": "4.4.*@dev",
        "symfony/dependency-injection": "4.4.*@dev",
        "symfony/twig-bridge": "4.4.*@dev",
        "symfony/http-kernel": "4.4.*@dev",
        "symfony/cache": "4.4.*@dev",
        "symfony/error-renderer": "4.4.*@dev",
        "symfony/http-foundation": "4.4.*@dev",
        "symfony/debug": "4.4.*@dev",
        "symfony/error-handler": "4.4.*@dev",
        "symfony/event-dispatcher": "4.4.*@dev",
        "symfony/config": "4.4.*@dev",
        "symfony/intl": "4.4.*@dev",
        "symfony/var-exporter": "4.4.*@dev",
        "symfony/mime": "4.4.*@dev",
        "symfony/doctrine-bridge": "4.4.*@dev",
        "symfony/options-resolver": "4.4.*@dev",
        "symfony/property-access": "4.4.*@dev",
        "symfony/filesystem": "4.4.*@dev",
        "symfony/security-csrf": "4.4.*@dev",
        "symfony/routing": "4.4.*@dev",
        "symfony/finder": "4.4.*@dev",
        "symfony/security-guard": "4.4.*@dev",
        "symfony/inflector": "4.4.*@dev",
        "symfony/security-core": "4.4.*@dev",
        "symfony/templating": "4.4.*@dev",
        "symfony/security-http": "4.4.*@dev",
        "symfony/monolog-bridge": "4.4.*@dev",
        "symfony/property-info": "4.4.*@dev",
        "symfony/serializer": "4.4.*@dev",
        "symfony/debug-bundle": "4.4.*@dev",
        "symfony/var-dumper": "4.4.*@dev",
        "symfony/stopwatch": "4.4.*@dev",
        "symfony/web-profiler-bundle": "4.4.*@dev",
        "symfony/css-selector": "4.4.*@dev",
        "symfony/dom-crawler": "4.4.*@dev",
        "symfony/browser-kit": "4.4.*@dev"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
